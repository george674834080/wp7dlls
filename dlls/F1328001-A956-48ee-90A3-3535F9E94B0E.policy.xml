<CeSecurityPolicyFile Description="Defines ID_CAP_DEBUG for emulator" Vendor="Microsoft" RequiredOSVersion="7.00" FileVersion="7.4.9876" xmlns="urn:Microsoft.PlatformBuilder/CeSecurityPolicy">


  <!-- Debug capability for debug apps running on emulator --> 
  
<!--Begin generating capability accounts and authorization rules for ID_CAP_DEBUG -->
<Macro Id="CAPMACRO_ID_CAP_DEBUG" Value="S-1-5-112-0-0X70-0X49445F4341505F4445425547" />
<Account Id="$(CAPMACRO_ID_CAP_DEBUG)" FriendlyName="ID_CAP_DEBUG:Allow an application to run in debugging enviorment." Description="Autogenerated group for capability ID_CAP_DEBUG" Type="Group">
<MemberOfGroup GroupAccountId="$(PUBLIC_CAPABILITIES_GROUP_NAME)" />
</Account>
<Rule Description="Authorization rule for capability ID_CAP_DEBUG" ResourceIri="$(HKCU)/SOFTWARE/MICROSOFT/ConMan/(*)" SpeakerAccountId="$(SYSTEM_USER_NAME)" PriorityCategoryId="PRIORITY_STANDARD">
<Authorize>
<Match AccountId="$(CAPMACRO_ID_CAP_DEBUG)" AuthorizationIds="KEY_ALL_ACCESS" />
</Authorize>
</Rule>
<Rule Description="Authorization rule for capability ID_CAP_DEBUG" ResourceIri="/FILESYS/PRIMARY/APPLICATION%20DATA/PHONE%20TOOLS/10.0/CORECON/LIB/(*)" SpeakerAccountId="$(SYSTEM_USER_NAME)" PriorityCategoryId="PRIORITY_STANDARD">
<Authorize>
<Match AccountId="$(CAPMACRO_ID_CAP_DEBUG)" AuthorizationIds="FILE_GENERIC_READ, FILE_GENERIC_EXECUTE" />
</Authorize>
</Rule>
<Rule Description="Authorization rule for capability ID_CAP_DEBUG" ResourceIri="$(LOADERVERIFIER_GLOBAL)/AUTHORIZATION/CERT_AUTHZ/(+)/(+)" SpeakerAccountId="$(SYSTEM_USER_NAME)" PriorityCategoryId="PRIORITY_STANDARD">
<Authorize>
<Match AccountId="$(CAPMACRO_ID_CAP_DEBUG)" AuthorizationIds="LV_ACCESS_LOAD" />
</Authorize>
</Rule>
<!--End generating capability accounts and authorization rules for ID_CAP_DEBUG -->


</CeSecurityPolicyFile>
