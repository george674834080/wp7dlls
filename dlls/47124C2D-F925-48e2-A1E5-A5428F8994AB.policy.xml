  <!-- Protect MapiShellNotification message queue from everyone -->
  <Rule Action="CreateOrReplace" Description="MapiShellNotification Message Queues are accessible to OM" ResourceIri="policy:/KERNEL/MSGQUEUE/$(OM_USER_NAME)/CommsApps/Messaging/ShellNotification/(*)" SpeakerAccountId="$(SYSTEM_USER_NAME)" PriorityCategoryId="PRIORITY_STANDARD">
