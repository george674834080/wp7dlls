      <Match Action="Add" AccountId="$(ELEVATED_RIGHTS_CHAMBER_GROUP_NAME)" AuthorizationIds="GENERIC_READ"></Match>
    </Authorize>
    <Stop>
      <Match AccountId="$(EVERYONE_GROUP_NAME)"></Match>
