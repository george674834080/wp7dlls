  <!-- 
  By default, taskhost & headlesshost run in an LPC chamber with  networking and sensors.
  This provides enough access to use the common functionality in 
  Silverlight and NETCF.
  -->
  
<!--Begin generating In-ROM application accounts and authorization rules for TASKHOST_CHAMBER_SID -->
<Macro Id="TASKHOST_CHAMBER_SID" Value="S-1-5-112-0-0X80-0X00000001" />
<Account Id="$(TASKHOST_CHAMBER_SID)" FriendlyName="TASKHOST LPC" Description="Autogenerated chamber for in-ROM LPC TASKHOST_CHAMBER_SID" Type="User">
<MemberOfGroup GroupAccountId="$(LEAST_PRIVILEGE_CHAMBER_GROUP_NAME)" />
<MemberOfGroup GroupAccountId="$(CAPMACRO_ID_CAP_NETWORKING)" />
<MemberOfGroup GroupAccountId="$(CAPMACRO_ID_CAP_SENSORS)" />
</Account>
<!--Begin Generated routing/authz rules for taskhost.exe -->
<Rule Description="Route taskhost.exe" ResourceIri="$(LOADERVERIFIER_ROUTE_BY_NAME)/PRIMARY/WINDOWS/TASKHOST.EXE" SpeakerAccountId="$(SYSTEM_USER_NAME)" PriorityCategoryId="PRIORITY_LOW">
<Authorize>
<Match AccountId="$(TASKHOST_CHAMBER_SID)" AuthorizationIds="LV_ACCESS_EXECUTE" />
</Authorize>
</Rule>
<Rule Description="Authorize taskhost.exe be loadable to $(TASKHOST_CHAMBER_SID)" ResourceIri="$(LOADERVERIFIER_EXE_AUTHZ_INROM_ROOT)/WINDOWS/TASKHOST.EXE" SpeakerAccountId="$(SYSTEM_USER_NAME)" PriorityCategoryId="PRIORITY_HIGH">
<Authorize>
<Match AccountId="$(TASKHOST_CHAMBER_SID)" AuthorizationIds="LV_ACCESS_EXECUTE,LV_ACCESS_LOAD" />
</Authorize>
<Stop>
<Match AccountId="$(EVERYONE_GROUP_NAME)" />
</Stop>
</Rule>
<!--End Generated routing/authz rules for taskhost.exe -->

<!--Begin Generated routing/authz rules for headlesshost.exe -->
<Rule Description="Route headlesshost.exe" ResourceIri="$(LOADERVERIFIER_ROUTE_BY_NAME)/PRIMARY/WINDOWS/HEADLESSHOST.EXE" SpeakerAccountId="$(SYSTEM_USER_NAME)" PriorityCategoryId="PRIORITY_LOW">
