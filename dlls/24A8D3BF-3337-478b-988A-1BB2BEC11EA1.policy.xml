<CeSecurityPolicyFile Description="Security policy for DeviceFeedback package" Vendor="Microsoft" RequiredOSVersion="7.00" FileVersion="7.00" xmlns="urn:Microsoft.PlatformBuilder/CeSecurityPolicy">  

  
<!--Begin Generated accounts and routing/authz rules for S-1-5-112-0-0X10-0X0000000B -->
<Macro Id="DEVICEFEEDBACKCPL_EXE_SID" Value="S-1-5-112-0-0X10-0X0000000B" />
<Account Id="$(DEVICEFEEDBACKCPL_EXE_SID)" FriendlyName="Device Feedback CPL" Description="Autogenerated chamber for Device Feedback CPL" Type="User">
<MemberOfGroup GroupAccountId="$(ELEVATED_RIGHTS_CHAMBER_GROUP_NAME)" />
</Account>
<!--Begin Generated routing/authz rules for DeviceFeedbackCPL.exe -->
<Rule Description="Route DeviceFeedbackCPL.exe" ResourceIri="$(LOADERVERIFIER_ROUTE_BY_NAME)/PRIMARY/WINDOWS/DEVICEFEEDBACKCPL.EXE" SpeakerAccountId="$(SYSTEM_USER_NAME)" PriorityCategoryId="PRIORITY_LOW">
<Authorize>
<Match AccountId="$(DEVICEFEEDBACKCPL_EXE_SID)" AuthorizationIds="LV_ACCESS_EXECUTE" />
</Authorize>
</Rule>
<Rule Description="Authorize DeviceFeedbackCPL.exe be loadable to $(DEVICEFEEDBACKCPL_EXE_SID) and  chambers" ResourceIri="$(LOADERVERIFIER_EXE_AUTHZ_INROM_ROOT)/WINDOWS/DEVICEFEEDBACKCPL.EXE" SpeakerAccountId="$(SYSTEM_USER_NAME)" PriorityCategoryId="PRIORITY_STANDARD">
<Authorize>
<Match AccountId="$(DEVICEFEEDBACKCPL_EXE_SID)" AuthorizationIds="LV_ACCESS_EXECUTE,LV_ACCESS_LOAD" />
</Authorize>
</Rule>
<!--End Generated routing/authz rules for DeviceFeedbackCPL.exe -->
<!--End Generated accounts and routing/authz rules for S-1-5-112-0-0X10-0X0000000B -->


  <Rule Description="Protecting UploadEnabled reg key" ResourceIri="$(HKLM)/SYSTEM/DeviceFeedback2/(*)" SpeakerAccountId="$(SYSTEM_USER_NAME)" PriorityCategoryId="PRIORITY_STANDARD">
    <Authorize>
      <Match AccountId="$(ELEVATED_RIGHTS_RESOURCE_GROUP_NAME)" AuthorizationIds="KEY_ALL_ACCESS"></Match>
      <Match AccountId="$(EVERYONE_GROUP_NAME)" AuthorizationIds="KEY_READ"></Match>
    </Authorize>
    <Stop>
      <Match AccountId="$(EVERYONE_GROUP_NAME)"></Match>
    </Stop>
  </Rule>

  <Rule Description="DumpFiles dir can be read by any signed app" ResourceIri="$(WINDOWS_DIRECTORY_PATH)/System/DumpFiles/(*)" SpeakerAccountId="$(SYSTEM_USER_NAME)" PriorityCategoryId="PRIORITY_ELEVATED">
    <Authorize>
      <Match AccountId="$(ELEVATED_RIGHTS_RESOURCE_GROUP_NAME)" AuthorizationIds="FILE_GENERIC_READ"></Match>
    </Authorize>
    <Stop>
      <Match AccountId="$(EVERYONE_GROUP_NAME)"></Match>
    </Stop>
  </Rule>

  <Rule Description="Allow normal apps to add logs to the non-ETW folder" ResourceIri="$(WINDOWS_DIRECTORY_PATH)/NonEtw_Logs/(*)" SpeakerAccountId="$(SYSTEM_USER_NAME)" PriorityCategoryId="PRIORITY_STANDARD">
    <Authorize>
      <Match AccountId="$(STANDARD_RIGHTS_RESOURCE_GROUP_NAME)" AuthorizationIds="FILE_ALL_ACCESS"></Match>
    </Authorize>
    <Stop>
      <Match AccountId="$(EVERYONE_GROUP_NAME)"></Match>
    </Stop>
  </Rule>

</CeSecurityPolicyFile>
