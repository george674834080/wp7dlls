<!--Begin Generated accounts and routing/authz rules for S-1-5-112-0-0X10-0X00000030 -->
<Macro Id="KEYBOARDCPL_EXE_SID" Value="S-1-5-112-0-0X10-0X00000030" />
<Account Id="$(KEYBOARDCPL_EXE_SID)" FriendlyName="KEYBOARDCPL.EXE" Description="Autogenerated chamber for KEYBOARDCPL.EXE" Type="User">
