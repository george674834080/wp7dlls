<!--Begin Generated routing/authz rules for KEYBOARDCPL.EXE -->
<Rule Description="Route KEYBOARDCPL.EXE" ResourceIri="$(LOADERVERIFIER_ROUTE_BY_NAME)/PRIMARY/WINDOWS/KEYBOARDCPL.EXE" SpeakerAccountId="$(SYSTEM_USER_NAME)" PriorityCategoryId="PRIORITY_LOW">
